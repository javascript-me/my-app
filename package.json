{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mocha": "3.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "classnames": "2.2.5",
    "less": "2.7.1",
    "chai": "3.5.0",
    "less-plugin-autoprefix": "1.5.1",
    "less-plugin-clean-css": "1.5.1",
    "enzyme": "^2.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "mocha src/**/*.spec.js --compilers js:babel-core/register --require src/setup.js",
    "css": "lessc --strict-imports --clean-css=\"\" --autoprefix=\"IE >= 11,last 5 Edge versions,last 10 Chrome versions\" --source-map --source-map-less-inline --source-map-rootpath=src src/index.less src/App.css",
    "build": "react-scripts build",
    "server": "serve -s build",

    "test0": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "istanbul": "^0.4.5"
  }
}
